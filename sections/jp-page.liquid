{{ 'jp-page.css' | asset_url | stylesheet_tag }}


<div class="page-wrapper">
  <div class="page">
    <h1 class="page-title">
      {%- if page.title != blank -%}
        {{ page.title | escape }}
      {%- else -%}
        Page title
      {%- endif -%}
    </h1>
    <div class="page-content">
      {%- if page.content != blank -%}
        {{ page.content }}
      {%- else -%}
        <div class='page-placeholder-wrapper placeholder'>
          {{ 'page' | placeholder_svg_tag: 'page-placeholder' }}
        </div>
      {%- endif -%}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "JP Page",
  "tag": "section",
  "class": "section",
  "settings": [
  ],
  "presets": [
    {
      "name": "JP Page"
    }
  ]
}
{% endschema %}
